	<SoundDef>
		<defName>Sangheili_Call</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>Pawn/Humanlike/Sangheili/Call</clipFolderPath>
